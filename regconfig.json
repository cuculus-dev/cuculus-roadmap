{
  "core": {
    "workingDir": ".reg",
    "actualDir": "directory_contains_actual_images",
    "thresholdRate": 0,
    "ximgdiff": {
      "invocationType": "client"
    }
  },
  "plugins": {
    "reg-keygen-git-hash-plugin": true,
    "reg-notify-github-plugin": {
      "prComment": true,
      "prCommentBehavior": "default",
      "clientId": "$REG_SUITE_CLIENT_ID"
    },
    "reg-notify-github-with-api-plugin": {
      "githubUrl": "https://github.com",
      "owner": "cuculus-dev",
      "repository": "cuculus-roadmap",
      "privateToken": "$GITHUB_TOKEN"
    },
    "reg-publish-s3-plugin": {
      "bucketName": "$REG_SUITE_S3_BUCKET_NAME",
      "acl": "private"
    }
  }
}
